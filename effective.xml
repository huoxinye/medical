int lost;
int lostn=4;
int list[np*3];//病人
int prit[np*3];//优先级
int ranklist[np*3];//类型
int tail=0; //队尾
bool rep[nn];//作为对照
//形成随机数
int rand(int n)
{
    int i=1;
    double j=random(n);
    while (i &lt; j)
    {
        i++;
    }
    return i-1;   
}
void delete(int p)
{
    int i=0;
    int j=0;
    for(i=0;i&lt;lptail;i++)
    {
        if (lplist[i]==p)
        {
            for(j=i;j&lt;lptail;j++)
                {
                    lplist[j]=lplist[j+1];
                }
                lptail=lptail-1;
                load[idn-1]++;
        }
    }

    for(i=0;i&lt;mptail;i++)
    {
        if (mplist[i]==p)
        {
            for(j=i;j&lt;mptail;j++)
                {
                    mplist[j]=mplist[j+1];
                }
                mptail=mptail-1;
                load[idn-1]++;
        }
    }

    for(i=0;i&lt;hptail;i++)
    {
        if (hplist[i]==p)
        {
            for(j=i;j&lt;hptail;j++)
                {
                    hplist[j]=hplist[j+1];
                }
                hptail=hptail-1;
                load[idn-1]++;
        }
    }

    for(i=0;i&lt;tail;i++)
    {
        if (list[i]==p)
        {
            for(j=i;j&lt;tail;j++)
                {
                    list[j]=list[j+1];
                    ranklist[j]=ranklist[j+1];
                    prit[j]=prit[j+1];
                }
                tail=tail-1;
                i=0;
        }
    }
}
void inque(int p,int r)
{
    int i,j,t,pri;
    int k=0;
    if(tail==lostn)
    {
        list[tail-1]=0;
        prit[tail-1]=0;
        ranklist[tail-1]=0;
        tail--;
        lost++;
     };//只能记住可记忆的长度
    for(i=0;i&lt;tail;i++)
    {
     if (list[i]==p and ranklist[i]==r)
        {
            for(j=i;j&lt;tail;j++)
            {
            list[i]=list[i+1];
            ranklist[i]=ranklist[i+1];
            prit[i]=prit[i+1];
            };
            tail=tail-1;
            pri=r+1;
        }   
    };//如果有重复的消息，则先把之前的那个去掉，然后把信息的优先级加1
     for(t=tail-1;t&gt;=-1;t--)
    {
        if(k==0 and (t==-1 or prit[t]&gt;=pri)) 
        {
            ranklist[t+1]=r;
            prit[t+1]=pri;
            list[t+1]=p;
            k = 1;     
        }
        if(t!=-1 and prit[t]&lt;pri) 
        {
            ranklist[t+1]=ranklist[t];
            prit[t+1]=prit[t];
            list[t+1]=list[t];
        } 
    }//加进去新消息以后排优先级  
    tail=tail+1;
}
bool need(int p)
{
bool ok=0;
int i;
for(i=0;i&lt;hptail;i++)
    {
    if(hplist[i]==p)
        {
        ok=1;
        }
    }
for(i=0;i&lt;mptail;i++)
    {
    if(mplist[i]==p)
        {
        ok=1;
        }
    }
for(i=0;i&lt;lptail;i++)
    {
    if(lplist[i]==p)
        {
        ok=1;
        }
    }
return ok;
}

