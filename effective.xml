		<declaration>const int inter=3;
clock cl;
void inlist(int i,int p)
{
    if (i==1)
    {
        lplist[lptail]=p;
        lptail++;
    }
    if (i==2)
    {
        mplist[mptail]=p;
        mptail++;
    }
    if (i==3)
    {
        hplist[hptail]=p;
        hptail++;
    }
}

bool voiceok()
{
bool ok=1;
int i;
for(i=0;i&lt;nn;i++)
    {
    if(works[i]==0 and ini[i]==0)
        {
        ok=0;
        }
    }
return ok;
}</declaration>
