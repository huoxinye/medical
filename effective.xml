		<declaration>void initbusy(){
int i=0;
int j=0;
int k=0;
//初始化病人的看护者
    for (j=1;j&lt;nn;j++){
        for(i=0;i&lt;np/nn;i++)
        {
        psn[k]=j;
        k=k+1;
        }
    };
     while(k&lt;np)
    {
        psn[k]=j;
        k=k+1;
    }
//初始化护士的工作状态对比
    for(i=0;i&lt;nn;i++)
   {
       ini[i]=1;
       inith[i]=1;
       //place[i]=1;
    };
}</declaration>
